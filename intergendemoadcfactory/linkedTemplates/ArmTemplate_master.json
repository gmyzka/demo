{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "intergendemoadcfactory"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureDatabricks_IntergenDemo_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_IntergenDemo'"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"azureStorageGen2LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azureStorageGen2LinkedService'"
		},
		"azureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azureStorageLinkedService'"
		},
		"NationalGridESODatasets_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.nationalgrideso.com/backend/dataset/"
		},
		"azureStorageGen2LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://intergendemoadcadls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "intergendemoadcfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"AzureDatabricks_IntergenDemo_accessToken": {
						"value": "[parameters('AzureDatabricks_IntergenDemo_accessToken')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"azureStorageGen2LinkedService_accountKey": {
						"value": "[parameters('azureStorageGen2LinkedService_accountKey')]"
					},
					"azureStorageLinkedService_connectionString": {
						"value": "[parameters('azureStorageLinkedService_connectionString')]"
					},
					"NationalGridESODatasets_properties_typeProperties_url": {
						"value": "[parameters('NationalGridESODatasets_properties_typeProperties_url')]"
					},
					"azureStorageGen2LinkedService_properties_typeProperties_url": {
						"value": "[parameters('azureStorageGen2LinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "intergendemoadcfactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'intergendemoadcfactory_ArmTemplate_0')]"
			]
		}
	]
}